<?xml version="1.0" encoding="utf-8"?>
<TabbedPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="PrismTabExample.Views.MyTabbedPage"
    xmlns:local="clr-namespace:PrismTabExample.Views">
    <!--Pages can be added as references or inline-->
    <TabbedPage.Children>
        <NavigationPage Title="First">
            <x:Arguments>
                <local:Tab1Page></local:Tab1Page>
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Second">
            <x:Arguments>
                <local:Tab2Page></local:Tab2Page>
            </x:Arguments>
        </NavigationPage>
    </TabbedPage.Children>
</TabbedPage>
